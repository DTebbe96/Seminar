<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="AnchorPane" prefHeight="533.0" prefWidth="648.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="seriesoftubes.SeriesOfTubesController">
   <children>
      <HBox layoutX="6.0" layoutY="8.0" prefHeight="518.0" prefWidth="649.0">
         <children>
            <VBox alignment="CENTER" prefHeight="518.0" prefWidth="337.0">
               <children>
                  <CheckBox fx:id="host" mnemonicParsing="false" text="Host?" />
                  <Label fx:id="userIp" text="Ip Address" />
                  <TextField fx:id="ipAddressField" />
                  <Button mnemonicParsing="false" onAction="#handleStart" text="Start" />
                  <Button mnemonicParsing="false" onAction="#update" text="Update" />
                  <Button mnemonicParsing="false" onAction="#handleExit" text="Exit" />
               </children>
            </VBox>
            <TextArea fx:id="connectionOutput" editable="false" prefHeight="518.0" prefWidth="303.0" />
         </children>
      </HBox>
   </children>
</AnchorPane>
